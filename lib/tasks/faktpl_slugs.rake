  site = Site.find(33)

  urls.each do |slug|

    begin
      shop = Shop.find_by(slug: slug[0], site_id: site.id)
      shop.slug = slug[1]
      shop.slug_mutable = 1
      shops << shop
      rule = RedirectRule.new(
        site_id: site.id,
        source: '/kodyrabatowe/' + slug[0].dup,
        destination: '/kodyrabatowe/' + slug[1].dup,
        active: 1,
        request_environment_rules:
        [RequestEnvironmentRule.new(environment_key_name: 'SERVER_NAME',environment_value: site.hostname.dup)]
      );
      rule = rule.dup
      redirects << rule
    rescue Exception => e
      binding.pry
    end

  end

  binding.pry

